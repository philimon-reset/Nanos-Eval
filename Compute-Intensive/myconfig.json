{
  "RunConfig": {
    "Verbose": true,
    "Ports": ["8083"],
    "Accel": true,
    "CPUs": 4,
    "Memory": "2G"
  },
  "Args": ["script.py"],
  "MapDirs": {
    "./.venv/bin/python": "/.local/bin/python",
    "./.venv/lib": "/.local/lib"
  },
  "Debugflags": ["trace:pf,threadrun", "debugsyscalls"]
}




